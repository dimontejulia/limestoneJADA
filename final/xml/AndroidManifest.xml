<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.balant.diana.jada">

    <application
        android:allowBackup="true"
        android:icon="@drawable/jada"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addAmount"></activity>
        <activity android:name=".BudgetingPage"></activity>
        <activity android:name=".ViewActivity"></activity>
        <activity android:name=".addPurchase"></activity>
    </application>

</manifest>